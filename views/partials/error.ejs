<% if (typeof errorMsg !== "undefined") { %>
<div class="flex justify-center items-center mt-5">
  <div
    class="text-black w-5/6 bg-red-200 px-10 p-2 flex justify-between items-center rounded border border-white"
  >
    <p class="break-words"><%= errorMsg %> </p>
    <p class="text-red-600 text-xl hidden">&#10006;</p>
  </div>
</div>
<% } %>
